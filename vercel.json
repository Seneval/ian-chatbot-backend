{
  "version": 2,
  "name": "ian-chatbot-backend",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "regions": ["iad1"]
}